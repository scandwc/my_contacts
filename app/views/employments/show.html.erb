<div class="page-header">
  <h3>Employment #<%= @employment.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Company</dt>
      <dd><%= @employment.company_id %></dd>

      <dt>Contact</dt>
      <dd><%= @employment.contact_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/employments" class="btn btn-primary">
        Back
      </a>
      <a href="/employments/<%= @employment.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_employment/<%= @employment.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
